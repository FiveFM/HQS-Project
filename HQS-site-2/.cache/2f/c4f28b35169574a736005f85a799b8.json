le to the page name\n    document.getElementById('top-app-bar-title').innerHTML = page;\n\n    //Jquery function that dynamically loads x.html into the main content of the page by id\n    $(function () {\n        //on build add vapor-store\n        if (process.env.NODE_ENV === 'development') {\n            $('#main-content').load(path.join('..', `/${page.toLowerCase().replace(/ /g, '_')}.html`));\n        } else {\n            $('#main-content').load(path.join('..', `vapor-store/${page.toLowerCase().replace(/ /g, '_')}.html`));\n        }\n    });\n\n    try {\n        window.mdc.autoInit();\n    } catch (e) {}\n};\ngoto();\n"},"lineCount":null}},"error":null,"hash":"38369da6b2ffd07132131a3eff0d992c","cacheData":{"env":{"NODE_ENV":"development"}}}